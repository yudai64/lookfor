<h1>ユーザー一覧</h1>
<table class="table">
  <thead>
    <tr>
      <th>名前</th>
      <th>一言</th>
      <th>ライン</th>
    </tr>
  </thead>
    <% @users.each do |user| %>
    <tbody>
      <tr>
        <td><%= link_to user.name, "/users/#{user.id} "%></td>
        <td><%= user.word %></td>
        <td><%= user.url %></td>
      </tr>
    </tbody>
    <% end %>
</table>
